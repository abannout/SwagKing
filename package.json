{
  "name": "player-hackschnitzel",
  "module": "es2022",
  "target": "es2017",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --es-module-specifier-resolution=node dist/index.js",
    "compile": "tsc --removeComments true",
    "dev": "npm run watch",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --ext .ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "watch-node": "nodemon --on-change-only --es-module-specifier-resolution=node dist/index.js",
    "watch-ts": "tsc -w",
    "watch": "concurrently --kill-others -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.10.3",
    "axios": "^1.1.3",
    "graphviz-wasm": "^3.0.0",
    "pino": "^8.7.0",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "concurrently": "^7.5.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.5"
  }
}
