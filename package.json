{
  "name": "player-hackschnitzel",
  "module": "es2022",
  "target": "es2017",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --es-module-specifier-resolution=node dist/index.js",
    "compile": "tsc --sourceMap false --removeComments true",
    "dev": "npm run watch",
    "watch-node": "nodemon --on-change-only --es-module-specifier-resolution=node dist/index.js",
    "watch-ts": "tsc -w",
    "watch": "concurrently --kill-others -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.10.3",
    "axios": "^1.1.3",
    "pino": "^8.7.0",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/node": "^18.11.0",
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20"
  }
}
